using System;

class MainClass {
  public static void Main (string[] args) 
  {
    double raio, volume;
    const double pi = 3.14159;

    raio = Convert.ToDouble(Console.ReadLine());

    volume =  (4 * pi * Math.Pow(raio , 3)) / 3;

    Console.WriteLine("VOLUME = "+Math.Round(volume, 3));
  }
}
